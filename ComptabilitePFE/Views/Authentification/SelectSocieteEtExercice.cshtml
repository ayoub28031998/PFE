@model List<Societe>

<h4>Société</h4>
<fieldset class="form-group position-relative has-icon-left mb-0">
    <select class="form-control" id="societesSelect">
        <option>-- Societe --</option>
        @if (Model != null)
        {
            foreach (var societe in Model)
            {
                <option value="@societe.CodeSociete">@societe.RaisonSociete</option>
            }
        }
    </select>
</fieldset>

<h4>Exercice</h4>
<fieldset class="form-group position-relative has-icon-left">
    <select class="form-control" id="ExerciceSelect" disabled>
    </select>
</fieldset>

<button type="button" id="addSocieteButton" class="btn btn-info btn-block" onclick="addSocieteButtonClicked()">Ajouter Société</button>
<button type="button" id="addExerciceButton" class="btn btn-info btn-block" onclick="addExerciceButtonClicked()" disabled>Ajouter Exercice</button>
<button type="button" id="continueButton" class="btn btn-info btn-block" onclick="continueButtonClicked()">Continuer</button>